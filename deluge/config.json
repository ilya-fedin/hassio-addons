{
  "name": "Deluge",
  "version": "dev",
  "slug": "deluge",
  "description": "Deluge is a lightweight, Free Software, cross-platform BitTorrent client (with telegram bot)",
  "_url": "",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "ingress": true,
  "ingress_port": 0,
  "panel_icon": "mdi:water",
  "startup": "services",
  "homeassistant": "0.92.0b2",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "ports": {
    "80/tcp": null
  },
  "ports_description": {
    "80/tcp": "Web interface (Not required for Hass.io Ingress)"
  },
  "boot": "auto",
  "hassio_api": true,
  "hassio_role": "default",
  "auth_api": true,
  "host_network": true,
  "map": [
    "ssl",
    "share:rw"
  ],
  "options": {
    "deluge_uid": 1000,
    "deluge_gid": 1000,
    "deluged_port": 58846,
    "telegram_token": "",
    "telegram_master": "",
    "telegram_webui_password": "deluge",
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)?",
    "deluge_uid": "int?",
    "deluge_gid": "int?",
    "deluged_port": "int",
    "telegram_proxy": "str?",
    "telegram_token": "str",
    "telegram_master": "str",
    "telegram_webui_password": "str",
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str",
    "leave_front_door_open": "bool?"
  }
}
